{
  "name": "frr-form",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Luke Zirngibl",
  "version": "2.0.3",
  "keywords": [],
  "scripts": {
    "clean": "rm -rf node_modules/@material-ui/core node_modules/@material-ui/styles node_modules/react node_modules/react-dom node_modules/react-redux node_modules/styled-components node_modules/frr-web",
    "link-frr": "yarn link frr-web",
    "start:example": "react-scripts start ./example/index.ts",
    "build": "tsc",
    "build:watch": "yarn link-frr && && yarn build --watch",
    "babel:build": "yarn clean && yarn link-frr && babel ./src --out-dir lib --extensions '.ts','.tsx','.js','.jsx'",
    "babel:watch": "yarn babel:build --watch",
    "build-types": "install-peers -f && tsc",
    "patch": "yarn run build-types && git add -A && git commit -m 'patch' && npm version patch",
    "prepare": "install-peers",
    "sync-frr-web": "rm -rf ./node_modules/frr-web/lib  && cp -r ../frr-web/lib ./node_modules/frr-web/lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukezirngibl/frr-form"
  },
  "files": [
    "lib/**/*"
  ],
  "peerDependencies": {
    "@material-ui/core": "^4.9.10",
    "fp-ts": "1.19.5",
    "frr-util": "^1.0.3",
    "frr-web": "2.0.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.0.0",
    "styled-components": "^5.0.0"
  },
  "dependencies": {
    "monocle-ts": "2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/styled-components": "^5.0.0",
    "eslint": "^7.21.0",
    "install-peers-cli": "^2.2.0",
    "prettier": "^2.2.1",
    "react-scripts": "^3.4.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.7.2"
  }
}
